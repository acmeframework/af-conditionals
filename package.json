{
    "name": "af-conditionals",
    "version": "0.1.0",
    "description": "A simple conditional library.",
    "main": "build/lib/index.js",
    "scripts": {
        "test": "./node_modules/.bin/nyc ./node_modules/.bin/mocha"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "**/*.d.ts",
            "./build/**/*",
            "./node_modules/**/*",
            "./reports/**/*",
            "./test/**/*"
        ],
        "reporter": [
            "html"
        ],
        "all": true,
        "report-dir": "./reports/coverage"
    },
    "author": "Mike Coakley https://github.com/mcoakley",
    "repository": {
        "type": "git",
        "url": "https://github.com/acmeframework/af-conditionals"
    },
    "license": "MIT",
    "dependencies": {},
    "devDependencies": {
        "@types/chai": "^4.1.4",
        "@types/mocha": "^5.2.4",
        "@types/node": "^10.7.1",
        "@types/sinon": "^5.0.1",
        "chai": "^4.1.2",
        "mocha": "^5.2.0",
        "mochawesome": "^3.0.3",
        "nyc": "^12.0.2",
        "sinon": "^6.1.5",
        "source-map-support": "^0.5.8",
        "tap-nyc": "^1.0.3",
        "ts-node": "^7.0.1",
        "typescript": "^3.0.1"
    }
}
